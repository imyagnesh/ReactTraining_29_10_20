{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": ["plugin:react/recommended", "plugin:prettier/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "react-hooks", "import", "jsx-a11y", "prettier"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
